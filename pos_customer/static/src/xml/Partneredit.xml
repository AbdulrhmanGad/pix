<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="PartnerDetailsEdits" t-inherit="point_of_sale.PartnerDetailsEdit" t-inherit-mode="extension" owl="1">
       <xpath expr="//div[@class='partner-details-right']" position="inside">
           <div class="partner-detail">
                        <span class="label">Gender</span>
                        <input class="detail gender" name="gender" type="selection" t-model="changes.gender" t-on-change="captureChange" placeholder ="gender" />
                    </div>
           <div class="partner-detail">
                        <span class="label">ID</span>
                        <input class="detail customerid" name="customerid"  t-model="changes.customerid" t-on-change="captureChange" placeholder ="customerid" />
                    </div>

       </xpath>

    </t>
</templates>